<%- include('../../shared/includes/head', {pageTitle: 'Beats'})  %>
<link rel="stylesheet" href="/styles/beats.css">
</head>
<body>
    <%- include('../../shared/includes/header') %>
    <main>
        <h1>Beat Administration</h1>
        <section>
            <h2>Manage Beats</h2>
            <p>
                <a href="/admin/beats/new" class="btn">Add Beat</a>
            </p>
        </section>
        <section>
            <ol id="beats-grid">
                <% for (const beat of beats) { %>
                <li>
                  <%- include('includes/beat-item' , { beat: beat} )%>
                </li>
                <% } %>
            </ol>
        </section>
    </main>
<%- include('../../shared/includes/footer') %>